{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "build:docker": "docker build . --tag=dev/start-screen",
    "dev": "cross-env NODE_ENV=development remix dev",
    "dev:worker": "node ./worker/worker.mjs",
    "dev:docker": "docker run -d --env-file .env.docker --name start-screen-dev -p 3000:3000 dev/start-screen",
    "dev:docker:stop": "docker stop start-screen-dev && docker rm start-screen-dev",
    "remix:dev": "remix dev",
    "postinstall": "remix setup node",
    "start": "concurrently \"npm:start:app\" \"npm:start:worker\"",
    "start:app": "cross-env NODE_ENV=production remix-serve ./build/index.js",
    "start:worker": "cross-env NODE_ENV=production node ./worker/worker.mjs",
    "docker": "prisma migrate deploy && prisma generate && npm run start",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "typecheck": "tsc --skipLibCheck",
    "prettier": "prettier app --check --end-of-line auto"
  },
  "dependencies": {
    "@arcath/utils": "^3.14.1",
    "@prisma/client": "^5.3.1",
    "@remix-run/node": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "@remix-run/serve": "^2.0.0",
    "@uiw/react-textarea-code-editor": "^2.1.7",
    "adm-zip": "^0.5.10",
    "bullmq": "^4.11.0",
    "color-string": "^1.9.1",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "ldapjs": "^3.0.5",
    "mdx-bundler": "^9.2.1",
    "mkdirp": "^3.0.1",
    "net-snmp": "^3.9.7",
    "node-cron": "^3.0.2",
    "node-unifi": "^2.4.1",
    "prisma": "^5.3.1",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.0",
    "@remix-run/eslint-config": "^2.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/adm-zip": "^0.5.1",
    "@types/color-string": "^1.5.3",
    "@types/cookie": "^0.5.2",
    "@types/kerberos": "^1.1.3",
    "@types/ldapjs": "^3.0.2",
    "@types/node": "^20.6.2",
    "@types/node-cron": "^3.0.8",
    "@types/qrcode": "^1.5.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.4",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "server-build",
    "build",
    "public/build",
    "*.ignored/",
    "*.ignored.*",
    "remix.env.d.ts",
    "docs"
  ]
}
