{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "build:docker": "docker build . --tag=dev/start-screen",
    "dev": "cross-env NODE_ENV=development remix dev",
    "dev:docker": "docker run -d --env-file .env.docker --name start-screen-dev -p 3000:3000 dev/start-screen",
    "dev:docker:stop": "docker stop start-screen-dev && docker rm start-screen-dev",
    "remix:dev": "remix dev",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "docker": "prisma migrate deploy && prisma generate && npm run start",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "typecheck": "tsc --skipLibCheck"
  },
  "dependencies": {
    "@arcath/utils": "^3.14.1",
    "@prisma/client": "^5.0.0",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/serve": "^1.19.1",
    "@uiw/react-textarea-code-editor": "^2.1.7",
    "adm-zip": "^0.5.10",
    "color-string": "^1.9.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.15.0",
    "ldapjs": "^3.0.4",
    "mdx-bundler": "^9.2.1",
    "mkdirp": "^3.0.1",
    "net-snmp": "^3.9.7",
    "node-unifi": "^2.4.1",
    "prisma": "^5.0.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.19.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/adm-zip": "^0.5.0",
    "@types/color-string": "^1.5.2",
    "@types/cookie": "^0.5.1",
    "@types/kerberos": "^1.1.2",
    "@types/ldapjs": "^3.0.0",
    "@types/node": "^18.16.3",
    "@types/qrcode": "^1.5.1",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "server-build",
    "build",
    "public/build",
    "*.ignored/",
    "*.ignored.*",
    "remix.env.d.ts",
    "docs"
  ]
}
