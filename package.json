{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "build:docker": "docker build . --tag=dev/start-screen",
    "dev": "cross-env NODE_ENV=development remix dev --manual -c \"node --watch-path ./server/index.js --watch ./server/index.js\"",
    "dev:worker": "node ./worker/worker.mjs",
    "dev:docker": "docker run -d --env-file .env.docker --name start-screen-dev -p 3000:3000 dev/start-screen",
    "dev:docker:stop": "docker stop start-screen-dev && docker rm start-screen-dev",
    "remix:dev": "remix dev",
    "postinstall": "remix setup node",
    "start": "concurrently \"npm:start:app\" \"npm:start:worker\"",
    "start:app": "cross-env NODE_ENV=production node ./server/index.js",
    "start:worker": "cross-env NODE_ENV=production node ./worker/worker.mjs",
    "docker": "prisma migrate deploy && prisma generate && npm run start",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "typecheck": "tsc --skipLibCheck",
    "prettier": "prettier app --check --end-of-line auto"
  },
  "dependencies": {
    "@arcath/utils": "^3.15.0",
    "@prisma/client": "^5.22.0",
    "@remix-run/architect": "^2.14.0",
    "@remix-run/node": "^2.14.0",
    "@remix-run/react": "^2.14.0",
    "@remix-run/serve": "^2.14.0",
    "@uiw/react-textarea-code-editor": "^3.1.0",
    "adm-zip": "^0.5.16",
    "bullmq": "^5.26.0",
    "color-string": "^1.9.1",
    "compression": "^1.7.5",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "dashjs": "^4.7.4",
    "date-fns": "^4.1.0",
    "esbuild": "^0.24.0",
    "express": "^4.21.1",
    "framer-motion": "^11.11.15",
    "ldapjs": "^3.0.7",
    "mdx-bundler": "^10.0.3",
    "mkdirp": "^3.0.1",
    "morgan": "^1.10.0",
    "net-snmp": "^3.15.1",
    "node-cron": "^3.0.3",
    "node-unifi": "^2.5.1",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix": "^2.14.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "source-map-support": "^0.5.21",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.14.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/adm-zip": "^0.5.6",
    "@types/color-string": "^1.5.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/kerberos": "^1.1.5",
    "@types/ldapjs": "^3.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/source-map-support": "^0.5.10",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "server-build",
    "build",
    "public/build",
    "*.ignored/",
    "*.ignored.*",
    "remix.env.d.ts",
    "docs"
  ]
}
